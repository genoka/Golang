//maximum et minimum en go
package main

import "fmt"

func Maximum( a int64,b int64) (max int64, bool )
{
var max int64
if a==b {
 return a, true
 }else{
 if a>b {
  return a , true}
  else{ return b,true }
  }
}

func Minimum(a int64, b int64) (min int64, bool )
{
var min int64
if a==b {
return a, true
}else{
if a>b {
   return b,true
    }else{
    return a,true
    }
  }
}

